{
  "realm": "samltenant",
  "enabled": true,
  "displayName": "SAML Example Tenant",
  "users": [
    {
      "username": "samluser",
      "email": "user@samltenant.example.com",
      "enabled": true,
      "credentials": [{
        "type": "password",
        "value": "samlpass",
        "temporary": false
      }]
    }
  ],
  "clients": [
    {
      "clientId": "conceptor-saml",
      "enabled": true,
      "protocol": "saml",
      "redirectUris": ["https://samltenant.example.com/*"],
      "attributes": {
        "saml.signing.certificate": "",
        "saml.signature.algorithm": "RSA_SHA256",
        "saml_force_name_id_format": "true",
        "saml_name_id_format": "email"
      }
    }
  ]
}
